# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /list                       de.stema.finance.DataController.list

GET     /data                       de.stema.finance.DataController.getData

GET     /writeJson                  de.stema.finance.DataController.writeJson(path: String)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
